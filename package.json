{
  "name": "ppc-landing-pages",
  "description":
    "A marketing landing page built with Gatsby and styled with Tailwind",
  "version": "1.0.0",
  "author": "American Addiction Centers",
  "dependencies": {
    "autoprefixer": "^9.1.1",
    "eslint": "^4.9.0",
    "gatsby": "^1.9.270",
    "gatsby-link": "^1.6.23",
    "gatsby-plugin-google-tagmanager": "^1.0.17",
    "gatsby-plugin-postcss": "^1.0.0",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "postcss-cli": "^5.0.0",
    "precss": "^3.1.2",
    "purgecss-webpack-plugin": "^1.2.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": ["gatsby", "tailwindcss", "purgecss"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build:css": "postcss ./src/styles/index.css -o ./src/index.css",
    "watch:css": "postcss ./src/styles/index.css -o ./src/index.css -w",
    "dev": "npm run watch:css & gatsby develop",
    "build": "npm run build:css && gatsby build",
    "build:prod": "npm run build:css && gatsby build --prefix-paths",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gatsby-cli": "^1.1.11",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.7.4",
    "purgecss": "^0.21.0",
    "tailwindcss": "^0.5.3"
  }
}
